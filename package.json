{
  "name": "EKMC-Web",
  "version": "0.0.18",
  "private": true,
  "dependencies": {
    "@cometchat-pro/chat": "^1.8.14",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@zeit/next-css": "^1.0.1",
    "actstore": "0.0.12",
    "clik": "file:./src/@clik",
    "markdown-to-jsx": "^6.9.1",
    "next": "^9.2.1",
    "next-fucss": "^0.1.9",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-particles-js": "^2.6.0",
    "react-spring": "^7.2.8",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "auto-changelog": "^1.14.1",
    "chromedriver": "^76.0.0",
    "geckodriver": "^1.16.2",
    "next-transpile-modules": "^2.3.1",
    "nightwatch": "^1.1.13",
    "prettier": "^1.18.2",
    "selenium-server": "^3.141.59",
    "serve": "^11.1.0"
  },
  "scripts": {
    "start": "ENV=test next -p 3000 -H 0.0.0.0",
    "start:win": "set ENV=test&& next -p 3001",
    "test": "nightwatch",
    "build": "next build",
    "export": "next export && node src/@clik/hooks/useStaticIndexCleaner.js",
    "serve": "rm -rf out && npm run build && npm run export && cd out && serve",
    "lint": "prettier --tab-width 2 --jsx-bracket-same-line true --single-quote --print-width 100 true --semi false --write \"src/**/*.js\"",
    "logs": "auto-changelog --ignore-commit-pattern '^(?!docs:)(?!test:)(?!fix:)(?!feat:)(?!style:)' -l false --sort-commits date-desc -p -o ./CHANGELOG.md && git add ./CHANGELOG.md"
  }
}
